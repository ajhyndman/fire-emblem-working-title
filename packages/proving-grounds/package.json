{
  "name": "proving-grounds",
  "version": "0.9.7",
  "description": "Test level 40 heroes from Fire Emblem: Heroes against one another!",
  "repository": "git@github.com:ajhyndman/fire-emblem-working-title.git",
  "author": "Andrew Hyndman <ajhyndman@hotmail.com>",
  "license": "GPL-3.0+",
  "private": true,
  "scripts": {
    "build": "npm run stamp-deploy && next build",
    "build-storybook": "build-storybook",
    "deploy": "now --npm",
    "dev": "next",
    "docker-build": "docker build -t proving-grounds .",
    "docker-start": "docker run -d -p 80:3000 proving-grounds",
    "lint": "eslint src/**/*.js pages/**/*.js && flow check",
    "stamp-deploy": "echo \"export const deployTimestamp = '$(date -I)';\" > deployTimestamp.js",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s .",
    "sync-static": "aws s3 sync ./static/cdn/ s3://proving-grounds-static.ajhyndman.com/ --cache-control max-age=2592000,public",
    "tape": "tape -r babel-register tests/**/*.js",
    "test": "npm run lint && npm run tape"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.17.1",
    "eslint-config-ajhyndman": "git+https://github.com/ajhyndman/eslint-config-ajhyndman#1.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "flow-bin": "^0.48.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "clipboard-js": "^0.3.3",
    "color-js": "^1.0.4",
    "crypto-js": "^3.1.9-1",
    "diacritics": "^1.3.0",
    "fire-emblem-heroes-calculator": "^0.9.7",
    "fire-emblem-heroes-stats": "^0.9.7",
    "lodash.throttle": "^4.1.1",
    "lz-string": "^1.4.4",
    "next": "2.0.0-beta.36",
    "next-redux-wrapper": "1.0.0",
    "now": "^4.10.0",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-icons": "^2.2.3",
    "react-redux": "^5.0.3",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-analytics": "^0.3.1"
  }
}
