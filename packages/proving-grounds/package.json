{
  "name": "proving-grounds",
  "version": "0.15.2",
  "description": "Test level 40 heroes from Fire Emblem: Heroes against one another!",
  "repository": "git@github.com:ajhyndman/fire-emblem-working-title.git",
  "author": "Andrew Hyndman <ajhyndman@hotmail.com>",
  "license": "GPL-3.0+",
  "private": true,
  "scripts": {
    "build": "yarn stamp-deploy && next build",
    "build-storybook": "build-storybook",
    "deploy": "now --npm --alias=proving-grounds.ajhyndman.com",
    "dev": "next",
    "docker-build": "docker build -t proving-grounds .",
    "docker-start": "docker run -d -p 80:3000 proving-grounds",
    "lint": "eslint src/**/*.js pages/**/*.js",
    "minify-images": "imagemin --plugin=pngquant static/*.png --out-dir=static && imagemin --plugin=pngquant static/cdn/*.png --out-dir=static/cdn",
    "prettier": "prettier --single-quote --trailing-comma=all --write src/**/*.js",
    "stamp-deploy": "echo \"export const deployTimestamp = '$(date +%Y-%m-%d)';\" > deployTimestamp.js",
    "start": "NODE_ENV=production node server.js",
    "storybook": "start-storybook -p 6006 -s .",
    "sync-static": "aws s3 sync ./static/cdn/ s3://proving-grounds-static.ajhyndman.com/ --cache-control max-age=2592000,public",
    "tape": "tape -r babel-register tests/**/*.js",
    "test": "yarn lint && yarn tape"
  },
  "devDependencies": {
    "@storybook/react": "3.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.17.1",
    "eslint-config-ajhyndman": "git+https://github.com/ajhyndman/eslint-config-ajhyndman#1.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "imagemin-cli": "3.0.0",
    "imagemin-pngquant": "5.0.1",
    "now": "9.2.7",
    "prettier": "^1.7.4",
    "sw-precache-webpack-plugin": "0.9.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "clipboard-js": "^0.3.3",
    "color-js": "^1.0.4",
    "crypto-js": "^3.1.9-1",
    "diacritics": "^1.3.0",
    "fire-emblem-heroes-calculator": "^0.15.2",
    "fire-emblem-heroes-stats": "^0.15.2",
    "lodash.throttle": "^4.1.1",
    "lz-string": "^1.4.4",
    "next": "8.1.0",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "ramda": "^0.23.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-ga": "^2.1.2",
    "react-icons": "^2.2.3",
    "react-redux": "^5.0.3",
    "recompose": "^0.22.0",
    "redux": "^3.6.0",
    "redux-analytics": "^0.3.1"
  }
}
