{
  "name": "fire-emblem-heroes-calculator",
  "version": "0.15.2",
  "description": "Calculate the result of combat between one hero and another",
  "main": "dist/fire-emblem-heroes-calculator.js",
  "author": "Andrew Hyndman <ajhyndman@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn compile && yarn compile:min && yarn copy-types",
    "compile": "rollup --config --environment BUILD:development",
    "compile:min": "rollup --config --environment BUILD:production",
    "compile:watch": "yarn compile -- --watch",
    "copy-types": "ncp src/index.js dist/fire-emblem-heroes-calculator.js.flow && jscodeshift dist/fire-emblem-heroes-calculator.js.flow --transform build/transform-import-path.js",
    "lint": "eslint src/**/*.js",
    "prepublish": "yarn build",
    "publish": "cross-env aws s3 sync ./dist/ s3://proving-grounds-static.ajhyndman.com/$npm_package_version/ --cache-control max-age=31536000,public",
    "start": "yarn build:watch",
    "tape": "tape -r @babel/register tests/**/*.js",
    "test": "yarn lint && yarn tape"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-transform-flow-strip-types": "7.4.4",
    "@babel/preset-env": "7.4.3",
    "@babel/register": "7.4.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-ramda": "^1.2.0",
    "chokidar-cli": "^1.2.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-ajhyndman": "2.0.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-flowtype": "3.6.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "jscodeshift": "0.6.3",
    "ncp": "^2.0.0",
    "prettier": "^1.17.0",
    "rollup": "1.10.1",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
    "rollup-plugin-uglify": "6.0.2",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "diacritics": "^1.3.0",
    "fire-emblem-heroes-stats": "^0.15.2",
    "ramda": "^0.23.0"
  }
}
